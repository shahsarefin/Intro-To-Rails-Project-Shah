<%= paginate @owners %>

<div class="container">
  <h1 class="text-center">Owners</h1>
  <table class="table">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Address</th>
      </tr>
    </thead>
    <tbody>
      <% @owners.each do |owner| %>
        <tr>
          <td><%= owner.id %></td>
          <td><%= owner.name %></td>
          <td><%= owner.phone_number %></td>
          <td><%= owner.email %></td>
          <td><%= owner.address %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<%= paginate @owners %>
